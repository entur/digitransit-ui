    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-relay-plugin": "^0.6.0",
    "body-parser": "^1.14.1",
    "classnames": "^2.2.1",
    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "fluxible": "^1.0.3",
    "fluxible-action-utils": "^0.2.4",
    "fluxible-addons-react": "^0.2.0",
    "foundation-apps": "^1.2.0",
    "history": "1.13.1",
    "javascript-natural-sort": "^0.7.1",
    "leaflet": "^1.0.0-beta.2",
    "lodash": "^3.10.1",
    "masonry": "geta6/masonry",
